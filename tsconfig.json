{
	"extends": "expo/tsconfig.base.json",
	"compilerOptions": {
		"strict": true,
		"jsx": "react-jsx",
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"paths": {
			"@/*": ["./*"]
		}
	},
	"include": [
		"**/*.ts",
		"**/*.tsx",
		"nativewind-env.d.ts",
		"../index.tsx",
		".expo/types/**/*.ts",
		"expo-env.d.ts"
	]
}
