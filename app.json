{
  "expo": {
    "name": "brie",
    "slug": "clientMobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "icon": "./assets/images/brie-logo.png",
    "splash": {
      "image": "./assets/images/brie-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#2BB252"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.xamata.brie",
      "icon": {
        "dark": "./assets/images/brie-logo.png",
        "light": "./assets/images/brie-logo.png"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/brie-logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.xamata.brie"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/brie-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/brie-icon.svg",
          "imageWidth": 150,
          "resizeMode": "contain",
          "backgroundColor": "#59A671",
          "dark": {
            "image": "./assets/images/brie-icon.svg",
            "imageWidth": 150,
            "resizeMode": "contain",
            "backgroundColor": "#000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "assetBundlePatterns": [
      "./assets/**/*"
    ],
    "extra": {
      "router": {},
      "API_URL": "http://localhost:3000",
      "eas": {
        "projectId": "5c37bf2f-48a8-4081-bddb-eb7ed5783845"
      }
    },
    "owner": "xamata"
  }
}
