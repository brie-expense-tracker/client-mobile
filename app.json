{
	"expo": {
		"name": "clientMobile",
		"slug": "clientMobile",
		"version": "1.0.0",
		"orientation": "portrait",
		"scheme": "myapp",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"icon": "./assets/images/splash-icon.png",
		"splash": {
			"image": "./assets/images/brie-icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#2BB252"
		},
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.xamata.brie",
			"icon": {
				"dark": "./assets/images/brie-icon.png",
				"light": "./assets/images/brie-icon.png"
			},
			"infoPlist": {
				"ITSAppUsesNonExemptEncryption": false
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/splash-icon.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.xamata.brie"
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "./assets/images/brie-icon.png",
					"imageWidth": 150,
					"resizeMode": "contain",
					"backgroundColor": "#59A671",
					"dark": {
						"image": "./assets/images/splash-icon.png",
						"imageWidth": 150,
						"resizeMode": "contain",
						"backgroundColor": "#000"
					}
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		},
		"assetBundlePatterns": ["./assets/**/*"],
		"extra": {
			"router": {},
			"eas": {
				"projectId": "5c37bf2f-48a8-4081-bddb-eb7ed5783845"
			}
		},
		"owner": "xamata"
	}
}
